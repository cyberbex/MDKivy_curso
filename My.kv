MI:
<MI>:
    orientation: "vertical"
    MDTopAppBar:
        id: toolbar    
        title: "My App"
        md_bg_color: app.theme_cls.primary_dark
        left_action_items:[["menu"]]
        right_action_items:[["clock"]]
        pos_hint:{"top":1}
   
    MDBoxLayout:
        orientation:"vertical"
        size_hint: .8, .2
        pos_hint:{"center_x":.5,"top":.8}
        MDTextField:
            id: password
            hint_text: "password"
    MDRaisedButton:
        text:"Click"
        pos:200,100
        on_release:app.maker()
         

    MDBottomAppBar:
        md_bg_color: app.theme_cls.primary_light
        MDTopAppBar:
            type: "bottom"
            title: "Bottom"
            icon: "android"
            mode: "end" 
            

